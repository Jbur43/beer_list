<%= form_for(@post) do |f| %>
	<%= f.label :beer_name %>
	<%= f.text_field :beer_name %>

	<br />

	<%= f.label :brewer %>
	<%= f.text_field :brewer %>

	<br />

	<%= f.label :city %>
	<%= f.text_field :city %>

	<br />

	<%= f.label :image %>
	<%= f.file_field :avatar %>

	<br />

	<%= f.submit %>
<% end %>